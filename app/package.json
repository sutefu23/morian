{
  "name": "frourio-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "cd .. && husky install app/.husky",
    "dev": "npm run migrate:dev && run-p dev:*",
    "dev:client": "next dev -p 8000 --hostname 0.0.0.0",
    "dev:server": "npm run dev --prefix server",
    "dev:aspida": "aspida --watch",
    "build": "run-p build:client build:server",
    "prebuild:client": "npm run generate",
    "build:client": "next build && next export",
    "build:server": "npm run build --prefix server",
    "generate": "run-p generate:*",
    "generate:aspida": "aspida",
    "generate:server": "npm run generate --prefix server",
    "lint": "eslint --ext .ts,.js,.tsx . && prettier --check \"./**/*.ts\"",
    "lint:fix": "eslint --ext .ts,.js,.tsx . --fix && prettier --write \"./**/*.ts\"",
    "migrate:dev": "npm run migrate:dev --prefix server",
    "migrate:reset": "npm run migrate:reset --prefix server",
    "start": "run-p start:*",
    "start:client": "next start",
    "start:server": "npm start --prefix server",
    "pretest": "npm run generate",
    "test": "jest",
    "pretypecheck": "npm run generate",
    "typecheck": "tsc --noEmit && tsc --noEmit -p server"
  },
  "dependencies": {
    "@aspida/axios": "^1.14.0",
    "@aspida/react-query": "^1.7.0",
    "@chakra-ui/color-mode": "^2.0.4",
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@prisma/client": "^5.3.1",
    "@react-pdf/renderer": "^3.1.12",
    "@types/react-dom": "^18.0.5",
    "axios": "^1.5.1",
    "class-validator": "^0.13.1",
    "dayjs": "^1.11.0",
    "decimal.js": "^10.3.1",
    "eslint-config-next": "^13.5.1",
    "exceljs": "^4.3.0",
    "framer-motion": "^10.16.4",
    "immer": "^9.0.12",
    "jsbarcode": "^3.11.5",
    "next": "^13.5.1",
    "nookies": "^2.5.2",
    "prisma": "^5.3.1",
    "react": "^18.2.0",
    "react-aria": "^3.12.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.2.0",
    "react-query": "^3.13.11",
    "react-scripts": "^5.0.0",
    "react-stately": "^3.11.0",
    "recoil": "^0.7.7",
    "recoil-persist": "^4.2.0"
  },
  "devDependencies": {
    "@fastify/static": "^5.0.2",
    "@testing-library/react": "^11.2.6",
    "@types/fastify-static": "^2.2.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "babel-jest": "^26.6.3",
    "core-js": "^3.22.7",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "frourio": "^1.0.0",
    "husky": "^8.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "next-intercept-stdout": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^5.2.2",
    "webpack-cli": "^4.9.2",
    "yarnlock-dedupe-check": "^2.1.1"
  },
  "volta": {
    "node": "18.18.0",
    "npm": "9.8.1"
  }
}
